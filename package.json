{
    "name": "smart-desk",
    "private": true,
    "type": "module",
    "version": "v1.0.0-preview.1",
    "scripts": {
        "prepare-release": "deno run -ERWN --allow-run https://raw.githubusercontent.com/EngagedAgility/Infrastructure/v1.0.0-preview.9/.github/cicd-scripts/prepare-for-release.ts",
        "force-status-checks": "git commit --allow-empty -m \"ci: force status checks\" && git push",
        "push-skip-ci": "git commit --allow-empty -m \"[skip ci] and push to origin\" && git push",
        "dev-front-end": "next dev",
        "dev": "pnpm install && pnpm tauri dev",
        "build-ui": "pnpm install && next build",
        "build-back-end": "cargo build --manifest-path \"./src-tauri/Cargo.toml\"",
        "rebuild-back-end": "pnpm clean-back-end && pnpm build-back-end",
        "build-dev": "pnpm install && pnpm tauri build --debug",
        "build-prod": "pnpm install && pnpm tauri build",
        "check-cicd": "deno check ./.github/cicd-scripts/*.ts",
        "start": "next start",
        "lint-ui": "next lint",
        "lint-back-end": "cargo fmt --manifest-path \"./src-tauri/Cargo.toml\" --check",
        "lint-cicd": "deno lint ./.github/cicd-scripts",
        "fmt-cicd": "deno fmt ./.github/cicd-scripts",
        "fmt-check-cicd": "deno fmt --check ./.github/cicd-scripts",
        "check-code": "tsc --noEmit",
        "check-all": "pnpm install && pnpm check-code && pnpm lint-ui && pnpm lint-back-end && pnpm lint-cicd && pnpm fmt-check-cicd",
        "clean-front-end": "deno run -RW ./dev-scripts/clean-front-end.ts",
        "clean-back-end": "cargo clean --manifest-path \"./src-tauri/Cargo.toml\"",
        "clean-all": "pnpm run clean-front-end && pnpm run clean-back-end",
        "reinstall-deps": "pnpm clean-all && pnpm install",
        "rebuild-prod": "pnpm reinstall-deps && pnpm build-prod",
        "test": "vitest run",
        "test-server": "vitest --ui --api 1221"
    },
    "dependencies": {
        "@emotion/react": "^11.13.3",
        "@emotion/styled": "^11.13.0",
        "@fontsource/roboto": "^5.1.0",
        "@mdx-js/loader": "^3.0.1",
        "@mdx-js/react": "^3.1.0",
        "@mui/icons-material": "^6.1.6",
        "@mui/material": "^6.1.6",
        "@mui/x-data-grid": "^7.21.0",
        "@mui/x-date-pickers": "^7.21.0",
        "@next/mdx": "^14.2.6",
        "@supabase/ssr": "^0.5.0",
        "@supabase/supabase-js": "^2.45.6",
        "@tanstack/react-query": "^5.52.0",
        "@tauri-apps/api": "2.0.2",
        "@tauri-apps/plugin-clipboard-manager": "2.0.0",
        "@tauri-apps/plugin-fs": "2.0.1",
        "@tauri-apps/plugin-process": "2.0.0",
        "@tauri-apps/plugin-shell": "2.0.1",
        "@tauri-apps/plugin-store": "2.1.0",
        "@tauri-apps/plugin-updater": "2.0.0",
        "@types/mdx": "^2.0.13",
        "dayjs": "^1.11.13",
        "next": "^14.2.6",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "sonner": "^1.5.0",
        "zustand": "^5.0.1"
    },
    "devDependencies": {
        "@tauri-apps/cli": "2.1.0",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.0.1",
        "@testing-library/user-event": "^14.5.2",
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^22.9.0",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^8.13.0",
        "@typescript-eslint/parser": "^8.2.0",
        "@vitest/ui": "^2.0.5",
        "chalk": "^5.3.0",
        "eslint": "^8.57.0",
        "eslint-config-next": "15.0.3",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.0.0",
        "fs-extra": "^11.2.0",
        "happy-dom": "^15.0.0",
        "path": "^0.12.7",
        "postcss": "^8.4.41",
        "tailwindcss": "^3.4.14",
        "typescript": "^5.6.3",
        "vite-tsconfig-paths": "^5.1.0",
        "vitest": "^2.0.5"
    }
}
