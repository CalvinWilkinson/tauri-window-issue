{
	"files.eol": "\n",
	"window.title": "${activeRepositoryName} - [${activeRepositoryBranchName}]",
	"workbench.colorCustomizations": {
		"titleBar.activeBackground": "#24c9db31",
	},
	"workbench.editor.customLabels.patterns": {
		"**/cicd-scripts/*.ts": "${filename} (CICD-SCRIPTS)",
		"**/workflows/*.yml": "${filename} (WORKFLOW)",
		"**/src-tauri/**/*.rs": "${filename} (TAURI)",
		"**/app/components/*.tsx": "${filename} (COMP)",
		"**/app/**/page.tsx": "${dirname} (PAGE)",
	},
	"deno.enablePaths": [
		".github/cicd-scripts",
		"./dev-scripts",
	],
	"deno.config": "./deno.json",
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"node_modules": true,
		"out": true,
		".next": true,
		"**/target": true,
		"github-actions-output.txt": true,
		"**/vitest.config.ts.timestamp-*": true,
		"tsconfig.tsbuildinfo": true,
	},
	"editor.detectIndentation": false,
	"files.associations": {
		"*.yml": "yaml",
		"*.yaml": "yaml",
		"*.env-*": "toml",
	},
	"[typescript]": {
		"editor.insertSpaces": false,
		"editor.tabSize": 3,
		"typescript.preferences.quoteStyle": "double"
	},
	"[javascript]": {
		"editor.insertSpaces": false,
		"editor.tabSize": 3,
		"javascript.preferences.quoteStyle": "double"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "vscode.typescript-language-features",
		"editor.insertSpaces": false,
		"editor.tabSize": 3,
	},
	"[yaml]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2,
	},
	"[jsonc]": {
		"editor.insertSpaces": false,
		"editor.tabSize": 4,
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"[github-actions-workflow]": {
		"editor.tabSize": 2,
		"editor.insertSpaces": true
	},
	"css.lint.unknownAtRules": "ignore",
	"eslint.enable": true,
	"eslint.validate": [
		"typescript",
		"typescriptreact"
	],
	"deno.enable": true,
	"vitest.rootConfig": "vitest.config.ts",
	"cSpell.words": [
		"aarch",
		"anagment",
		"APPCONFIG",
		"appimage",
		"APPLOG",
		"autodetection",
		"bgcolor",
		"bucketid",
		"bucketname",
		"cheduling",
		"CICD",
		"clippy",
		"Deno",
		"denoland",
		"dmin",
		"dtolnay",
		"endgroup",
		"eporting",
		"ERWN",
		"fkey",
		"fontsource",
		"fullscreen",
		"hideable",
		"icns",
		"kdadmin",
		"Kinson",
		"kinsondigital",
		"libappindicator",
		"libgtk",
		"libjavascriptcoregtk",
		"librsvg",
		"libsoup",
		"libwebkit",
		"nextjs",
		"Nlcjg",
		"nsis",
		"patchelf",
		"Postgrest",
		"pwsh",
		"reauthentication",
		"RELEASETYPE",
		"REPONAME",
		"roboto",
		"runneradmin",
		"sonner",
		"sortcolumn",
		"sortorder",
		"supa",
		"supabase",
		"tailwindcss",
		"tanstack",
		"Tauri",
		"testid",
		"Unlisten",
		"unoptimized",
		"Unsubscriber",
		"USERPROFILE",
		"vitest",
		"webkitgtk",
		"zustand"
	],
}
